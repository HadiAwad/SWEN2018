pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
	    sh 'pwd'
                echo 'Building..'
		bat 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
